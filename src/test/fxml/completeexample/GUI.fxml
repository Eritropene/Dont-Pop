<VBox 
	xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="test.fxml.completeexample.UIController"
	stylesheets="test/fxml/styles.css"
	spacing="5">
	<padding>
		<Insets
			top="10"
			bottom="0"
			left="10"
			right="0" />
	</padding>
	
	<!-- Your Score -->
	<Label
		fx:id="yourScoreLabel"
		text="Your Score" />
	<TableView
		fx:id="yourScoreTable"
		prefHeight="200.0"
		prefWidth="200.0">
		<columns>
            <!--Rank-->
            <TableColumn 
            	fx:id="yourRankCol" 
                text="Rank" 
            </TableColumn>

            <!--Name-->
            <TableColumn 
            	fx:id="yourNameCol" 
                text="Name" 
            </TableColumn>

            <!--Score-->
            <TableColumn 
            	fx:id="yourScoreCol" 
                text="Score" 
            </TableColumn>
        </columns>
	</TableView>

	<!-- Leaderboard -->
	<Label
		fx:id="leaderboardLabel"
		text="Leaderboard" />

	<!-- Menu -->
	<Button
		fx:id="menuButton"
		text="Home"
		onAction="#buttonClicked" />
</VBox>
